roup(wpr))\nwpr_corall$Data <- 3\nwpr_corall <- left_join(wpr_corall, wpr_e %>% select(ws_num, wd_num, fejl = mae, fejl_p = mae), by = c(\"ws_num\", \"wd_num\"))\n\n# --------------------------------------------------------------------------- #\n# Samlet korrelation datafil\ncorall <- bind_rows(w_corall, wp_corall, wpr_corall)\n\n# --------------------------------------------------------------------------- #\n# --------------------------------------------------------------------------- #",
